	objssc.c sscconv.c sscasn2xml.c

OBJ=$(SRC:.c=.o)
EXE=sscasn2xml

CC=gcc -pipe
CFLAGS=-c -O3 -I .
LDFLAGS=-O3
RM=rm

.c.o :
	$(CC) $(CFLAGS) -c $<

.PHONY : all
all: $(EXE)

$(EXE): $(OBJ)
	$(CC) $(OBJ) $(LDFLAGS) -o $@

.PHONY : clean
clean:
	-$(RM) $(OBJ) core
